<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="student" class ="com.Autowiring.SI.byType.Student" autowire="byType" >
<property name="uid" value="1042"></property>
<property name="student_name" value="Manish Chaurasiya"></property>
<property name="age" value="25"></property>

</bean>
<bean id="addd" class="com.Autowiring.SI.byType.Address" scope="prototype">

<property name="Pincode" value="488442"></property>
<property name="StreetName" value="Jhanda Bazar"></property>
<property name="House_Number" value="920"></property>
<property name="city" value="Muhandra"></property>
<property name="state" value="MP"></property>

</bean>


</beans>