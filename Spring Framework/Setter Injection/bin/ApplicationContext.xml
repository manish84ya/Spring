<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- List Collection -->

	<bean id="list" class="com.Collection_Property_List.Employee">
		<property name="Ename">
			<list>
				<value>Manish</value>
				<value>Chandan</value>
				<value>Vishal</value>
				<value>Taksh</value>
				<value>Manish</value>
			</list>
		</property>
	</bean>


	<bean id="set" class="com.Collection_Property_Set.Colour">
		<property name="color">
			<set>
				<value>pink</value>
				<value>orange</value>
				<value>red</value>
				<value>red</value>
			</set>
		</property>
	</bean>
	
	<bean id="Map" class="com.Map_Property_Map.Student">
		<property name="name">
			<map>
				<entry key="1" value="Chaman" />
				<entry key="2" value="Raman" />
				<entry key="3" value="Pawan" />
				<entry key="4" value="Kalyan" />
			</map>
		</property>


	</bean>

</beans>